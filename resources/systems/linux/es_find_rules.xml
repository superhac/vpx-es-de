<?xml version="1.0"?>
<!-- This is the ES-DE find rules configuration file for Linux -->
<ruleList>
    <emulator name="OS-SHELL">
        <!-- Operating system shell -->
        <rule type="systempath">
            <entry>bash</entry>
            <entry>sh</entry>
        </rule>
    </emulator>
    <emulator name="VISUAL-PINBALL">
        <!-- Pinball simulator Visual Pinball -->
        <rule type="systempath">
            <entry>VPinballX_BGFX</entry>
        </rule>
        <rule type="staticpath">
            <entry>~/Applications/VPinballX/VPinballX_BGFX</entry>
        </rule>
    </emulator>
</ruleList>
